class PrimeMethod{
      public static int isPrime(int n){
      int count=0;
      for(int i=1;i<=num;i++){
      if(num%i==0){
      count++;
      }
      if(count==2)
      return1;
      else
      return0;
      }
      }
      public static void main(String[] args){
      for(int i=1;i<=100;i++){
      if(isPrime(i)==1)
      System.out.println(i);
      }
      }
      }
